
@{
	ViewBag.Title = "ComingSoon";
}

	<section class="flat-row flat-coming-soon parallax parallax2">
		<div class="overlay"></div>
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<div class="wrap-coming-soon text-center">
						<div class="header-coming-soon">
							<div class="title">Coming Soon</div>
						</div>
						<div class="wrap-countdown">
							<div class="countdown"></div>
						</div>
						<div class="button flat-button">
							<button onclick="window.location.href='@Url.Action("Index","Home")'" type="button" class="bg-white">BACK TO HOME</button>
							<button onclick="window.location.href='@Url.Action("Index","Shop")'" type="button">CONTINUE SHOPING</button>
						</div>
					</div><!-- /.wrap-coming-soon -->
				</div><!-- /.col-md-12 -->
			</div><!-- /.row -->
		</div><!-- /.container -->
	</section><!-- /.flat-row -->
